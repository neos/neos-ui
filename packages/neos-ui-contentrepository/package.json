  },
  "dependencies": {
    "@neos-project/neos-ui-extensibility": "1.0.14"
  },
  "license": "GNU GPLv3",
  "jest": {
    "preset": "@neos-project/jest-preset-neos-ui",
    "setupFiles": [
      "../../node_modules/@neos-project/jest-preset-neos-ui/src/setupNeosUiHostEnv.js"
    ]
