{
  "name": "@neos-project/neos-ui-views",
  "version": "1.0.13",
  "description": "Neos CMS UI Views for use in the inspector.",
  "main": "src/manifest.js",
  "private": true,
  "scripts": {
    "prepublish": "exit 0",
    "prebuild": "check-dependencies && yarn clean",
    "test": "yarn jest -- -w 2 --coverage",
    "test:watch": "yarn jest -- --watch",
    "clean": "rimraf ./lib ./dist",
    "lint": "eslint src && stylelint 'src/**/*.css'",
    "jest": "NODE_ENV=test jest",
    "postpublish": "greenkeeper-postpublish"
  },
  "devDependencies": {
    "@neos-project/babel-preset-neos-ui": "1.0.13",
    "@neos-project/build-essentials": "1.0.13",
    "@neos-project/debug-reason-for-rendering": "1.0.13",
    "@neos-project/jest-preset-neos-ui": "1.0.13"
  },
  "dependencies": {
    "@neos-project/brand": "^1.1.0",
    "@neos-project/neos-ui-extensibility": "1.0.13",
    "@neos-project/neos-ui-i18n": "1.0.13",
    "@neos-project/neos-ui-inspector": "1.0.13",
    "@neos-project/neos-ui-redux-store": "1.0.13",
    "@neos-project/react-ui-components": "1.0.13"
  },
  "license": "GNU GPLv3",
  "jest": {
    "preset": "@neos-project/jest-preset-neos-ui"
  }
